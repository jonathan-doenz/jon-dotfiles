{
  "load_extensions": {
    "notify/notify": true,
    "snippets/main": true,
    "codefolding/main": true,
    "snippets_menu/main": true,
    "comment-uncomment/main": true,
    "select_keymap/main": true,
    "varInspector/main": true,
    "collapsible_headings/main": true,
    "scratchpad/main": false,
    "nbextensions_configurator/config_menu/main": true,
    "contrib_nbextensions_help_item/main": true
  },
  "comment_uncomment_indent": true,
  "Cell": {
    "cm_config": {
      "lineNumbers": true
    }
  },
  "keys": {
    "command": {
      "bind": {
        "`": "jupyter-notebook:change-cell-to-code",
        "y,y": "jupyter-notebook:copy-cell",
        "shift-m": "jupyter-notebook:scroll-cell-center",
        "alt-v": "varInspector:toggle-variable-inspector",
        "m": "jupyter-notebook:merge-cells",
        "m,m": "jupyter-notebook:merge-selected-cells",
        "cmd-alt-b": "jupyter-notebook:run-all-cells-above",
        "cmd-alt-e": "jupyter-notebook:run-all-cells-below",
        "cmd-alt-r": "jupyter-notebook:run-all-cells",
        "shift-t": "jupyter-notebook:scroll-cell-top",
        "ctrl-k": "jupyter-notebook:select-previous-cell",
        "ctrl-j": "jupyter-notebook:select-next-cell"
      },
      "unbind": [
        "y",
        "c",
        "m"
      ]
    }
  }
}