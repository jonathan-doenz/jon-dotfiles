	\\uv{${1:${VISUAL:v}}}${0}
# Complex number underlined using following function (could remove this one if not needed anymore)
# \newcommand{\comp}[1]{\underline{#1}} 
snippet comp \comp{}
	\\comp{${1:${VISUAL:v}}}${0}
# Math operators
snippet abs absolute value operator \abs{}
	\\abs{${1:${VISUAL:v}}} ${0}
snippet norm norm operator \norm{}
	\\norm{${1:${VISUAL:v}}} ${0}
# \renewcommand{\=}[1]{\stackrel{#1}{=}} % for putting numbers above =
snippet =^ equal sign with above notation
	\\={${1:${VISUAL:v}}}${0}
snippet grad grad operator \grad{}
	\\grad{${1:${VISUAL:v}}}${0}
# \renewcommand{\div}[1]{\gv{\nabla} \cdot #1}
snippet div divergence operator \div{}
	\\div{${1:${VISUAL:v}}}${0}
# \newcommand{\curl}[1]{\gv{\nabla} \times #1}
snippet curl curl operator \curl{}
	\\curl{${1:${VISUAL:v}}}${0}
# \newcommand{\avg}[1]{\left< #1 \right>} % for average
snippet avg average operator \avg{}
	\\avg{${1:${VISUAL:v}}}${0}
# \renewcommand{\d}[2]{\frac{d #1}{d #2}} % for derivatives
snippet d derivative \d{}{}
	\\d{${1:${VISUAL:function}}}{${2:variable}}${0}
# \newcommand{\dd}[2]{\frac{d^2 #1}{d #2^2}} % for double derivatives
snippet dd double derivative \dd{}{}
	\\dd{${1:${VISUAL:function}}}{${2:variable}}${0}
# \newcommand{\pd}[2]{\frac{\partial #1}{\partial #2}} 
snippet pd partial derivative \pd{}{}
	\\pd{${1:${VISUAL:function}}}{${2:variable}}${0}
# \newcommand{\pdd}[2]{\frac{\partial^2 #1}{\partial #2^2}} 
snippet pdd partial double derivative \pdd{}{}
	\\pdd{${1:${VISUAL:function}}}{${2:variable}}${0}
# \newcommand{\pdc}[3]{\left( \frac{\partial #1}{\partial #2} \right)_{#3}} % for thermodynamic partial derivatives
snippet pdc partial derivative with constants \pdc{}{}{}
